    "d4c13e1f-6c89-415f-a385-7ef6a159aa98": {
      "type": "seo-section",
      "blocks": {
        "template--18732286083336__d4c13e1f-6c89-415f-a385-7ef6a159aa98-fullwidthblock-1": {
          "type": "fullwidthblock",
          "settings": {
            "full_width_heading": "",
            "full_width_description": "",
